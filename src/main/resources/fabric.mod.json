{
  "schemaVersion": 1,
  "id": "pridefishinglines",
  "version": "${version}",
  "name": "PrideFishingLine",
  "description": "Simple mixin mod to make the fishing line pride coloured",
  "authors": [
    "Badstagram"
  ],
  "contact": {
    "repo": "https://github.com/Badstagram/PrideFishingLine",
    "issues": "https://github.com/Badstagram/PrideFishingLine/issues",
    "homepage": "https://github.com/Badstagram/PrideFishingLine"
  },
  "license": "MIT",
  "icon": "assets/pridefishinglines/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "gay.badstagram.pridefishinglines.client.PrideFishingLineClient"
    ],
    "main": [
      "gay.badstagram.pridefishinglines.PrideFishingLine"
    ],
    "modmenu": [
      "gay.badstagram.pridefishinglines.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "pridefishinglines.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "1.20.*"
  },
  "recommends": {
    "yet_another_config_lib": "^0.83.0+1.20",
    "modmenu": "^7.0.1"
  }
}
